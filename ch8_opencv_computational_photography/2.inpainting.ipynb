    "import numpy as np\n",
    "import cv2 as cv\n",
    "\n",
    "\n",
    "img = cv.imread('../data/messi_2.jpg')\n",
    "\n",
    "mask = cv.imread('../data/mask2.png',0)\n",
    "\n",
    "# 两两张图裁切到一样大\n",
    "mask = mask[0:img.shape[0],0:img.shape[1]]\n",
    "\n",
    "dst = cv.inpaint(img,mask,3,cv.INPAINT_TELEA)\n",
    "cv.imshow('dst',dst)\n",
    "cv.waitKey(0)\n",
    "cv.destroyAllWindows()\n"
   ]
  }
